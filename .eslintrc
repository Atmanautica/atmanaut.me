{
  "extends": [
    "prettier",
    "eslint:recommended"
  ],
  "env": {
    "commonjs": true,
    "browser": true,
    "amd": true
  },
  "globals": {
    "domready": true,
    "Fuse": true
  },
  "rules": {
    "import/first": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  }
}
