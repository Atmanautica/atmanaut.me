{
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "commonjs": true,
    "browser": true,
    "amd": true
  },
  "plugins": [
    "prettier"
  ],
  "globals": {
    "domready": true,
    "Fuse": true
  },
  "rules": {
    "prettier/prettier": ["error", {}, {
      "usePrettierrc": true
    }],
    "import/first": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
